{
    "name": "opensynergic/payment-gateway-manager",
    "description": "manage for all payment gateway",
    "license": "MIT",
    "keywords": [
        "opensynergic",
        "laravel",
        "payment-gateway-manager"
    ],
    "autoload": {
        "psr-4": {
            "OpenSynergic\\PaymentGateway\\": "src/"
        }
    },
    "authors": [
        {
            "name": "mrickyansyah",
            "email": "mrickyansyah@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "calebporzio/sushi": "^2.4",
        "filament/filament": "^2.0",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSynergic\\PaymentGateway\\PaymentGatewayServiceProvider"
            ],
            "aliases": {
                "PaymentGateway": "OpenSynergic\\PaymentGateway\\Facades\\PaymentGateway"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
